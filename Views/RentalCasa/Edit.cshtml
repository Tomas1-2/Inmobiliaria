@model Inmobiliaria.Models.RentalCasa

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>

<h4>RentalCasa</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="RentalCasaId" />
            <div class="form-group">
                <label asp-for="FechaAlquiler" class="control-label"></label>
                <input asp-for="FechaAlquiler" class="form-control" />
                <span asp-validation-for="FechaAlquiler" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClienteId" class="control-label"></label>
                <select asp-for="ClienteId" class="form-control" asp-items="ViewBag.ClienteId"></select>
                <span asp-validation-for="ClienteId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CasaID" class="control-label"></label>
                <select asp-for="CasaID" class="form-control" asp-items="ViewBag.CasaID"></select>
                <span asp-validation-for="CasaID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
