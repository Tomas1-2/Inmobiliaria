@model IEnumerable<Inmobiliaria.Models.RentalCasa>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaAlquiler)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Casa)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaAlquiler)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente.clienteID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Casa.casaID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RentalCasaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RentalCasaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RentalCasaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
